<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="co.edu.javeriana.ciclipointer.ProgramarViajeActivity">

    <SearchView
        android:layout_width="389dp"
        android:layout_height="42dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="6dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:queryHint="Origen"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:id="@+id/searchView3" />

    <ListView
        android:id="@+id/listOpcionesViaje"
        android:layout_width="372dp"
        android:layout_height="486dp"
        android:layout_marginBottom="3dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteY="113dp" />

    <SearchView
        android:layout_width="384dp"
        android:layout_height="45dp"
        android:queryHint="Destino"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/searchView3"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/listOpcionesViaje"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintVertical_bias="0.0" />
</android.support.constraint.ConstraintLayout>
